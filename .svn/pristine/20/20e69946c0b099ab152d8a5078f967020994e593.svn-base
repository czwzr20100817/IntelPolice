package com.model;

import java.util.Date;

/**
 * 类名称：消息
 */
public class Message {
    // 消息ID ID
    private String id;
    // 删除状态 status
    private String status;
    // 接收人姓名 receiveUserName
    private String receiveUserName;
    // 接收人员id receiveUserid
    private String receiveUserId;
    // 消息内容 content
    private String content;
    // 创建时间 createTime
    private Date createTime;
    // 创建人员id createUserId
    private String createUserId;
    // 创建人员姓名 createUserName
    private String createUserName;
    // 是否查看 isVisit
    private String isVisit;
    // 消息标题 title
    private String title;
    // 查看时间 visitTime
    private Date visitTime;

    public Message() {
        super();
    }

    public Message(String id, String status, String receiveUserName, String receiveUserId, String content, Date createTime, String createUserId, String createUserName, String isVisit, String title, Date visitTime) {
        this.id = id;
        this.status = status;
        this.receiveUserName = receiveUserName;
        this.receiveUserId = receiveUserId;
        this.content = content;
        this.createTime = createTime;
        this.createUserId = createUserId;
        this.createUserName = createUserName;
        this.isVisit = isVisit;
        this.title = title;
        this.visitTime = visitTime;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getReceiveUserName() {
        return receiveUserName;
    }

    public void setReceiveUserName(String receiveUserName) {
        this.receiveUserName = receiveUserName;
    }

    public String getReceiveUserId() {
        return receiveUserId;
    }

    public void setReceiveUserId(String receiveUserId) {
        this.receiveUserId = receiveUserId;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public String getCreateUserId() {
        return createUserId;
    }

    public void setCreateUserId(String createUserId) {
        this.createUserId = createUserId;
    }

    public String getCreateUserName() {
        return createUserName;
    }

    public void setCreateUserName(String createUserName) {
        this.createUserName = createUserName;
    }

    public String getIsVisit() {
        return isVisit;
    }

    public void setIsVisit(String isVisit) {
        this.isVisit = isVisit;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public Date getVisitTime() {
        return visitTime;
    }

    public void setVisitTime(Date visitTime) {
        this.visitTime = visitTime;
    }

    @Override
    public String toString() {
        return "Message{" +
                "id='" + id + '\'' +
                ", status='" + status + '\'' +
                ", receiveUserName='" + receiveUserName + '\'' +
                ", receiveUserId='" + receiveUserId + '\'' +
                ", content='" + content + '\'' +
                ", createTime=" + createTime +
                ", createUserId='" + createUserId + '\'' +
                ", createUserName='" + createUserName + '\'' +
                ", isVisit='" + isVisit + '\'' +
                ", title='" + title + '\'' +
                ", visitTime=" + visitTime +
                '}';
    }
}