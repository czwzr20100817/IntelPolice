<%@ page language="java" pageEncoding="UTF-8" %>
<%@include file="/WEB-INF/views/common_prefix.jsp" %>
<div class="bjui-pageContent">
    <div class="pageFormContent" data-layout-h="0">
        <table class="table table-hover hw-table-long-input" width="100%">
            <tbody>
            <tr class="info">
                <td align="right" width="150px">
                    <label class="row-label">姓名：</label>
                </td>
                <td>
                    ${entity.name}
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">身份证号：</label>
                </td>
                <td>
                   ${entity.identityCard}
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">账户：</label>
                </td>
                <td>
                   ${entity.account}
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">警号：</label>
                </td>
                <td>
                   ${entity.policeNumber}
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">联系电话：</label>
                </td>
                <td>
                   ${entity.phone}
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">短号：</label>
                </td>
                <td>
                   ${entity.shortPhone}
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">性别：</label>
                </td>
                <td>
                    <hs:code value="${entity.sex}" field="SEX" />
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">人员类型：</label>
                </td>
                <td>
                    <hs:code value="${entity.userType}" field="USERTYPE" />
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">所属部门：</label>
                </td>
                <td>
                    <hs:dept value="${entity.deptId}"/>
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">锁定状态：</label>
                </td>
                <td>
                    <hs:code value="${entity.locked}" field="LOCKER" />
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">启用状态：</label>
                </td>
                <td>
                    <hs:code value="${entity.status}" field="ISSTRUTS" />
                </td>
            </tr>
            <tr>
                <td align="right" width="150px">
                    <label class="control-label">备注：</label>
                </td>
                <td>
                   ${entity.remark}
                </td>
            </tr>
            </tbody>
        </table>
    </div>
</div>
<div class="bjui-pageFooter">
    <ul>
        <li>
            <button type="button" class="btn btn-close" data-icon="close">取消</button>
        </li>

    </ul>
</div>