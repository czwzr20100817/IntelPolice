package com.utils;

import java.io.File;
import java.io.IOException;

import org.apache.commons.io.FileUtils;

/**
 * Created by Administrator on 2018/4/18 0018.
 */
public class FileUploadUtils {
    /**
     *      复制文件
     * @param file                      [必填]        数据文件
     * @param objFile                   [必填]        复制地址
     * @throws IOException
     */
    public static void upload(File file, File objFile) throws IOException{
        // 完成文件上传.
        FileUtils.copyFile(file,objFile);
    }
}
