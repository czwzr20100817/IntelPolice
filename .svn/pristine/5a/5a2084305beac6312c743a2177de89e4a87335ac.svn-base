package com.model.bean;

import java.util.ArrayList;
import java.util.List;

import com.model.Role;

/**
 * Created by Administrator on 2018/4/9 0009.
 */
public class RoleBean extends Role {
    private String deptName;
    private String funIds;
    List<String> permissoinIds = new ArrayList<>();
    List<String> menuIds = new ArrayList<>();

    public List<String> getPermissoinIds() {
        return permissoinIds;
    }

    public void setPermissoinIds(List<String> permissoinIds) {
        this.permissoinIds = permissoinIds;
    }

    public List<String> getMenuIds() {
        return menuIds;
    }

    public void setMenuIds(List<String> menuIds) {
        this.menuIds = menuIds;
    }

    public String getFunIds() {
        return funIds;
    }

    public void setFunIds(String funIds) {
        this.funIds = funIds;
    }

    public String getDeptName() {
        return deptName;
    }

    public void setDeptName(String deptName) {
        this.deptName = deptName;
    }
}
