    <select id="selectCriminalBeanByjjdbh" resultMap="BaseResultMap">
        SELECT distinct SJBH,CMC.* FROM CASE_M_CRIMINAL CMC
        INNER JOIN CASE_M_110CMD CM110C
        ON CMC.caseid = CM110C.caseid
