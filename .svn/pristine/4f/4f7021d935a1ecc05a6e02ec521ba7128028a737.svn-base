    <!--回告未成功-->
    <select id="selectSmsNotFsData" resultMap="BaseResultMap1">
        SELECT * FROM
        (SELECT ROWNUM AS rowno,cr.* FROM CASE_M_CRIMINAL cr
        <where>
            <if test="entity.caseid != null and entity.caseid != ''">
                AND cr.CASEID LIKE CONCAT(CONCAT('%', #{entity.caseid, jdbcType=VARCHAR}), '%')
            </if>
            <if test="entity.casename != null and entity.casename != ''">
                AND cr.CASENAME LIKE CONCAT(CONCAT('%', #{entity.casename, jdbcType=VARCHAR}), '%')
            </if>
            <if test="entity.alarmtype != null and entity.alarmtype != ''">
                AND cr.ALARMTYPE LIKE CONCAT(CONCAT('%', #{entity.alarmtype, jdbcType=VARCHAR}), '%')
            </if>
            AND ROWNUM &lt;= #{pageBean.pageCount}
            <if test="stuts == 'fs'">
                AND CASEID IN (SELECT CASEID FROM SMS_LOG)
            </if>
            <if test="stuts == 'wfs'">
                AND CASEID NOT IN (SELECT CASEID FROM SMS_LOG)
            </if>
            <if test="stuts == 'cf'">
                AND CASEID IN (SELECT CASEID FROM SMS_LOG group by CASEID having count(*) > 1)
            </if>
        </where>
        ORDER BY cr.CASEID
        ) table_alias
        <if test="stuts == 'fs'">
            inner join SMS_LOG sl
            on table_alias.CASEID = sl.CASEID
        </if>
        <if test="stuts == 'cf'">
            inner join SMS_LOG sl
            on table_alias.CASEID = sl.CASEID
        </if>
        WHERE table_alias.rowno >= #{pageBean.pageNum}
    </select>

