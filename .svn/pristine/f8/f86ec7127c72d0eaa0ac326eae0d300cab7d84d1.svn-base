<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.service.mapper.EaMediaCaseBeanMapper">
    <select id="selectByDocid" parameterType="java.lang.String" resultType="com.model.bean.EaMediaCaseBean">
    select  ID,MEDIAID,CASEID,JJDBH,CJDBH
    from EA_MEDIA_CASE
    where  MEDIAID = #{id}
    </select>

</mapper>